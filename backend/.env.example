# 필수 설정
SECRET_KEY=your-super-secret-key-at-least-32-characters-long
FERNET_KEY=your-fernet-key-generate-with-cryptography

# 데이터베이스 (Railway 제공 또는 SQLite)
DATABASE_URL=sqlite+aiosqlite:///./richer.db

# CORS (Vercel 프론트엔드 URL로 변경)
CORS_ORIGINS=["https://your-frontend.vercel.app"]

# 로그 설정
LOG_LEVEL=INFO
DEBUG=false

# 공개 시장 데이터 조회용 KIS API 설정 (선택적)
# 설정하면 계좌 등록 없이도 KOSPI/KOSDAQ 지수, 인기종목 조회 가능
KIS_APP_KEY=
KIS_APP_SECRET=
KIS_ACCOUNT_NO=
KIS_MOCK=true
